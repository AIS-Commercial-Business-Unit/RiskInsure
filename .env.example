# Environment variables for Docker Compose
# Copy this file to .env and fill in your actual connection strings

# Cosmos DB Connection String
COSMOSDB_CONNECTION_STRING=AccountEndpoint=https://riskinsure-cosmosdb.documents.azure.com:443/;AccountKey=YOUR_COSMOS_KEY_HERE;

# Azure Service Bus Connection String
SERVICEBUS_CONNECTION_STRING=Endpoint=sb://acmecorp-dev-servicebus.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=YOUR_SERVICE_BUS_KEY_HERE;
